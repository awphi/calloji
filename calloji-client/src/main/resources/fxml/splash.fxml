<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane minHeight="160.0" minWidth="348.0" prefHeight="160.0" prefWidth="348.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ph.adamw.calloji.client.gui.SplashController">
   <center>
      <VBox prefHeight="158.0" prefWidth="348.0" style="-fx-alignment: center;" BorderPane.alignment="CENTER">
         <children>
            <Label opacity="0.88" text="Server IP:" textAlignment="CENTER" textOverrun="CLIP">
               <font>
                  <Font name="System Bold" size="22.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="8.0" />
               </VBox.margin>
            </Label>
            <TextField onKeyReleased="#onIpKeyUp" fx:id="ipField" maxWidth="-Infinity" prefHeight="27.0" prefWidth="238.0">
               <VBox.margin>
                  <Insets bottom="8.0" />
               </VBox.margin>
            </TextField>
            <Button onAction="#onConnectButtonPressed" mnemonicParsing="false" text="Connect">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </center>
</BorderPane>
